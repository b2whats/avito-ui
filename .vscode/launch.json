  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach",
      "port": 43382,
      "request": "attach",
      "type": "pwa-node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/node_modules/.bin/styleguidist",
      "args": ["server"],
      "env": {
        "TARGET": "server"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Jest",
      "args": [
          "--config=${workspaceFolder}/server/jest.config.js",
          "--runInBand"
      ],
      "env": {
        "NODE_ENV": "test"
      },
      "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
  }
  ]
}