{
  "name": "avito-monorepo",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "cross-env TARGET=server styleguidist server",
    "build": "cross-env TARGET=server NODE_ENV=production styleguidist build",
    "bench": "cross-env BABEL_ENV=cjs TARGET=server NODE_ENV=production babel-node benchmark/index.js --extensions \".js,.ts,.tsx\"",
    "lint": "eslint ./packages/*/src --ext .js,.ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:snap": "jest '.snap.test.ts'",
    "test:unit": "jest '(?<!.snap).test.ts'",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "prerelease": "lerna version --no-push",
    "release": "lerna publish from-git --yes",
    "postrelease": "git push --follow-tags origin"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently 'yarn lint' 'yarn typecheck'"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@avito/ct": "^5.0.4",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "benchmark": "^2.1.4",
    "circular-dependency-plugin": "^5.0.2",
    "concurrently": "^5.1.0",
    "create-emotion-server": "^10.0.14",
    "cross-env": "^7.0.2",
    "deepmerge": "^4.0.0",
    "effector": "^20.15.6",
    "effector-react": "^20.8.0",
    "enhanced-resolve": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.3",
    "jest": "^26.0.1",
    "react": "^16.8.6",
    "react-docgen-typescript": "^1.12.5",
    "react-dom": "^16.8.6",
    "react-styleguidist": "^11.0.5",
    "typescript": "3.8.3",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=12.1.0"
  },
  "dependencies": {}
}
