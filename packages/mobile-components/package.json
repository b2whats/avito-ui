{
  "name": "@avito/mobile-components",
  "version": "0.0.20",
  "repository": {
    "type": "git",
    "url": "http://stash.msk.avito.ru/projects/MDS/repos/ui-web-components",
    "directory": "packages/mobile-components"
  }, 
  "private": false,
  "description": "Components for mobile web",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "cjs",
    "es",
    "types"
  ],
  "scripts": {
    "build": "yarn build:cjs & yarn build:es & yarn build:types",
    "build:types": "rm -rf types && tsc -p tsconfig.json --emitDeclarationOnly",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel src --out-dir cjs --extensions \".ts,.tsx\" --source-maps --root-mode=upward --delete-dir-on-start",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src --out-dir es --extensions \".ts,.tsx\" --source-maps --root-mode=upward --delete-dir-on-start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "yarn build",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@avito/core": "^0.0.17",
    "@avito/tokens": "^0.0.17"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "gitHead": "350c5d9160754ebc8a0d0a7b4fdf12aec2e90722"
}
