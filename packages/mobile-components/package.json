{
  "name": "@avito/mobile-components",
  "version": "0.0.33",
  "repository": {
    "type": "git",
    "url": "http://stash.msk.avito.ru/projects/MDS/repos/ui-web-components",
    "directory": "packages/mobile-components"
  },
  "private": false,
  "description": "Components for mobile web",
  "license": "MIT",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "typesVersions": {
    "*": {
      "cjs/*": [
        "types/*"
      ],
      "esm/*": [
        "types/*"
      ]
    }
  },
  "files": [
    "cjs",
    "esm",
    "types",
    "icons"
  ],
  "scripts": {
    "build": "yarn build:cjs & yarn build:esm & yarn build:types",
    "build:types": "rm -rf types && tsc -p tsconfig.json --emitDeclarationOnly",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel src --out-dir cjs --extensions \".ts,.tsx\" --source-maps --root-mode=upward --delete-dir-on-start",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel src --out-dir esm --extensions \".ts,.tsx\" --source-maps --root-mode=upward --delete-dir-on-start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "yarn build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@avito/core": "^0.0.30",
    "@avito/tokens": "^0.0.23"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "gitHead": "1207b77319e0691dc6276f1983ed2180eb5464f3"
}
