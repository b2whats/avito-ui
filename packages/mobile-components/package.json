{
  "name": "@avito/mobile-components",
  "version": "0.0.2",
  "private": false,
  "description": "Components for mobile web",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "browser": "build/index.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:js && yarn build:types",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir build --extensions \".ts,.tsx\" --source-maps --root-mode=upward",
    "clean": "rm -rf ./build && rm -rf tsconfig.tsbuildinfo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "yarn build",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@avito/core": "^0.0.1",
    "@avito/icons": "^0.0.5",
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.27",
    "create-emotion-server": "^10.0.14",
    "emotion": "^10.0.23",
    "fast-memoize": "^2.5.1",
    "react-gesture-responder": "^2.1.0",
    "scroll-into-view-if-needed": "^2.2.20",
    "touchable-hook": "^1.3.0"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "publishConfig": {
    "access": "restricted"
  },
