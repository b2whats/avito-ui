{
  "name": "@avito/mobile-components",
  "version": "0.0.9",
  "private": false,
  "description": "Components for mobile web",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:js && yarn build:types",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir build --extensions \".ts,.tsx\" --source-maps --root-mode=upward",
    "clean": "rm -rf ./build && rm -rf tsconfig.tsbuildinfo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "yarn build",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@avito/core": "^0.0.6",
    "@avito/icons": "^0.0.11",
    "@avito/tokens": "^0.0.11"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "publishConfig": {
    "access": "restricted"
  },
